<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.User" table="Users">
        <id name="id" column="id">
            <generator class="native"></generator>
        </id>

        <property name="reputation"></property>
        <property name="createDate"></property>
        <property name="displayName"></property>
        <property name="lastAccessDate"></property>
        <property name="webSiteUrl"></property>
        <property name="location"></property>
        <property name="abountMe"></property>
        <property name="views"></property>
        <property name="upVotes"></property>
        <property name="downVotes"></property>
        <property name="profileImageUrl"></property>
        <property name="emailHash"></property>
        <property name="password"></property>
        <property name="age"></property>
        <property name="accountId"></property>
    </class>
</hibernate-mapping>